    <!-- Light -->
    <font
        android:font="@font/airbnbcereal_light"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/airbnbcereal_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!-- bold -->
    <font
        android:font="@font/airbnbcereal_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/airbnbcereal_bold"
